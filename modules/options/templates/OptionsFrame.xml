<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Include file="OptionsFrame.lua" />
    <Frame name="MapPinEnhancedOptionsFrameTemplate" mixin="MapPinEnhancedOptionsFrameMixin"
        inherits="PortraitFrameTemplate, TabSystemOwnerTemplate"
        enableMouse="true" toplevel="true" parent="UIParent" movable="true" clampedToScreen="true"
        frameStrata="HIGH" hidden="true" virtual="true">
        <Size x="800" y="500" />
        <Anchors>
            <Anchor point="CENTER" relativePoint="CENTER" x="0" y="0" />
        </Anchors>
        <Frames>
            <Frame parentKey="TabSystem" inherits="TabSystemTemplate">
                <KeyValues>
                    <KeyValue key="minTabWidth" value="100" type="number" />
                    <KeyValue key="maxTabWidth" value="150" type="number" />
                    <KeyValue key="tabSelectSound" value="SOUNDKIT.UI_CLASS_TALENT_TAB"
                        type="global" />
                </KeyValues>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" x="22"
                        y="2" />
                </Anchors>
            </Frame>
            <Frame parentKey="optionsView" frameLevel="100"
                hidden="true">
                <Size x="793" y="473" />
                <Anchors>
                    <Anchor point="BOTTOM" x="0" y="4" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="bg" setAllPoints="true">
                            <Color a="0.1" r="0.0" g="0.0" b="0.9" />
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>

            <Frame parentKey="importView" frameLevel="100"
                hidden="true">
                <Size x="793" y="473" />
                <Anchors>
                    <Anchor point="BOTTOM" x="0" y="4" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="bg" setAllPoints="true">
                            <Color a="0.1" r="0.9" g="0.0" b="0.0" />
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>

            <Frame parentKey="seteditorView" frameLevel="100"
                hidden="true">
                <Size x="793" y="473" />
                <Anchors>
                    <Anchor point="BOTTOM" x="0" y="4" />
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="bg" setAllPoints="true">
                            <Color a="0.1" r="0.0" g="0.9" b="0.0" />
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad" />
        </Scripts>
    </Frame>
</Ui>
