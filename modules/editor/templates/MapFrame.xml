<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Include file="MapFrame.lua" />
    <Frame name="MapPinEnhancedMapTemplate" mixin="MapPinEnhancedMapMixin"
        inherits="MapCanvasFrameTemplate" virtual="true">
        <Frames>
            <ScrollFrame parentKey="ScrollContainer"
                inherits="MapCanvasFrameScrollContainerTemplate" setAllPoints="true"
            >
            </ScrollFrame>

            <Frame parentKey="BorderFrame" frameStrata="HIGH" setAllPoints="true">
                <Layers>
                    <Layer level="ARTWORK" textureSubLevel="5">
                        <Texture parentKey="Underlay" hidden="true">
                            <Color r="0" g="0" b="1" a=".5" />
                        </Texture>
                    </Layer>
                    <Layer level="BORDER" textureSubLevel="-1">
                        <Texture parentKey="border"
                            file="interface/talentframe/specialization">
                            <TextureSliceMode mode="Stretched" />
                            <TextureSliceMargins left="161" right="161"
                                top="101" bottom="101" />
                            <TexCoords left="0.00048828125" right="0.15771484375"
                                top="0.83837890625"
                                bottom="0.93701171875" />
                            <Anchors>
                                <Anchor point="TOPLEFT" x="-6" y="6" />
                                <Anchor point="BOTTOMRIGHT" x="6" y="-6" />
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>

        </Frames>
        <Scripts>
            <OnLoad method="OnLoad" />
            <OnShow method="OnShow" />
            <OnHide method="OnHide" />
            <OnEvent method="OnEvent" />
            <OnUpdate method="OnUpdate" />
        </Scripts>
    </Frame>
</Ui>
