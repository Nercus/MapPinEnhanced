<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Include file="MPHFrame.xml"/>
    <Include file="MPHMapPin.xml"/>
    <Include file="MPHMinimapPin.xml"/>
    <Include file="MPHTrackerEntry.xml"/>
    <Include file="MPHImportFrame.xml"/>
