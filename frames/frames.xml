                <AbsDimension x="1" y="-1"/>
            </Offset>
            <Color r="0" g="0" b="0"/>
        </Shadow>
    </Font>
    <Font height="14" name="MPHWhiteNormal" inherits="GameFontNormalLarge" virtual="true">
        <Color r="1.0" g="1.0" b="1.0"/>
        <Shadow>
            <Offset>
                <AbsDimension x="1" y="-1"/>
            </Offset>
            <Color r="0" g="0" b="0"/>
        </Shadow>
    </Font>
    <Font height="14" name="MPHYellowNormal" inherits="GameFontNormalLarge" virtual="true">
        <Color r="1.0" g="0.82" b="0"/>
        <Shadow>
            <Offset>
                <AbsDimension x="1" y="-1"/>
