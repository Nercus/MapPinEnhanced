{
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "cSpell.words": [
        "baseframe",
        "changedalpha",
        "CLAMPTOBLACKADDITIVE",
        "colorpicker",
        "Colour",
        "debugstack",
        "Desaturated",
        "dialogframe",
        "HBDP",
        "hooksecurefunc",
        "horiz",
        "Lerp",
        "MAPID",
        "MAPTYPE",
        "OPTIONCATEGORY",
        "texel",
        "toplevel",
        "uncategorized",
        "Unscrollable",
        "userwaypoint"
    ],
    "dryerLint.ruleSets": [
        {
            "glob": "{*.lua,**/!(libs|.hooks)/**/*.lua}",
            "language": "lua",
            "name": "WoW Addon Code Styling",
            "rules": [
                {
                    "fix": "",
                    "message": "Don't use print in production code.",
                    "name": "NoPrint",
                    "pattern": "\\bprint\\s*\\(",
                    "severity": "Error"
                },
            ]
        },
    ],
    "editor.formatOnSave": true,
    "editor.indentSize": "tabSize",
    "editor.tabCompletion": "onlySnippets",
    "editor.tabSize": 4,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": true,
    "explorer.fileNesting.patterns": {
        "*.lua": "${capture}.lua, ${capture}.xml",
        "*.toc": ".pkgmeta, .luarc.json, README.md, .gitignore, LICENSE, CHANGELOG.md, .pre-commit-config.yaml, install-deps.sh",
    },
    "explorer.sortOrder": "default",
    "Lua.completion.autoRequire": false,
    "Lua.diagnostics.disable": [],
    "Lua.diagnostics.groupFileStatus": {
        "ambiguity": "Any",
        "await": "Any",
        "duplicate": "Any",
        "global": "Any",
        "luadoc": "Any",
        "redefined": "Any",
        "strict": "Any",
        "strong": "Any",
        "type-check": "Any",
        "unbalanced": "Any",
        "unused": "Any"
    },
    "Lua.diagnostics.groupSeverity": {
        "global": "Error"
    },
    "Lua.diagnostics.libraryFiles": "Disable",
    "Lua.diagnostics.neededFileStatus": {
        "undefined-field": "Any!"
    },
    "Lua.format.defaultConfig": {
        "indent_size": "4",
        "indent_style": "space"
    },
    "Lua.hint.setType": false,
    "Lua.workspace.ignoreDir": [
        ".github",
        ".vscode",
        "libs",
        "_old"
    ],
    "material-icon-theme.folders.associations": {
        "panel": "Template",
        "pins": "Taskfile",
        "sets": "Queue",
        "tracker": "Ui"
    },
    "peacock.color": "#EBCB8B",
    "workbench.colorCustomizations": {
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#f2deb6",
        "statusBar.background": "#ebcb8b",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#e3b75f",
        "statusBarItem.remoteBackground": "#ebcb8b",
        "statusBarItem.remoteForeground": "#15202b",
        "tab.activeBorder": "#f2deb6",
        "titleBar.activeBackground": "#ebcb8b",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#ebcb8b99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "wowAPI.luals.defineKnownGlobals": false,
    "Lua.workspace.library": [
        "~\\.vscode\\extensions\\ketho.wow-api-0.21.0\\Annotations\\Core",
        "~\\.vscode\\extensions\\ketho.wow-api-0.21.0\\Annotations\\FrameXML"
    ],
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.builtin": {
        "basic": "disable",
        "debug": "disable",
        "io": "disable",
        "math": "disable",
        "os": "disable",
        "package": "disable",
        "string": "disable",
        "table": "disable",
        "utf8": "disable"
    },
    "Lua.type.weakUnionCheck": true,
}
