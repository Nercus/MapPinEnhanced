<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Script file="checkbox.lua" />

    <CheckButton name="MapPinEnhancedCheckboxTemplate" mixin="MapPinEnhancedCheckboxMixin"
        propagateMouseInput="Motion"
        virtual="true">
        <Size x="30" y="30" />
        <NormalTexture atlas="checkbox-minimal" />
        <PushedTexture atlas="checkbox-minimal" />
        <CheckedTexture atlas="checkmark-minimal" />
    </CheckButton>
</Ui>
