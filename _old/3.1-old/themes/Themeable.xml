<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Include file="Themeable.lua" />


    <Frame parent="UIParent" mixin="MapPinEnhancedThemeableMixin">
        <Size x="100" y="100" />

        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>


        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background">
                    <Size x="100" y="100" />
                    <Anchors>
                        <Anchor point="TOPLEFT" />
                        <Anchor point="BOTTOMRIGHT" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>


        <Scripts>
            <OnLoad>
                self:DefineTexture(self.background, "james", "interface/common/commoniconmask",
                "Default");
                self:DefineTexture(self.background, "james", "interface/common/commonmaskcircle",
                "ElvUI");
            </OnLoad>
        </Scripts>

    </Frame>
</Ui>
